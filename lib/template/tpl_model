"use strict";
var mongoose = require('mongoose');
var path = require('path');
var application = require('../conf/application');

var root= application.root;
var mongodb = require(path.join(path.dirname(root),'lib','mongodb'));
var schema = require('../schema/{model_name}');
var model=  mongoose.model('{model_name}',new mongoose.Schema(schema,{ versionKey: false }));

class {model_name} extends mongodb {
	constructor() {
    	super(model,application);
	}		
}

module.exports = {model_name};